<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.17.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="41" author="Eversis">
        <loadData file="db/init-data/content.csv" tableName="content" separator="|" />
    </changeSet>
    <changeSet id="42" author="Eversis">
        <loadData file="db/init-data/content-title.csv" tableName="content_title" separator="|" />
    </changeSet>
    <changeSet id="43" author="Eversis">
        <loadData file="db/init-data/content-data.csv" tableName="content_data" separator="|">
            <column header="content_id" name="content_id" type="BIGINT(20)"/>
            <column header="data" name="data" type="LONGTEXT"/>
            <column header="locale" name="locale" type="VARCHAR(255)"/>
        </loadData>
    </changeSet>
    <changeSet id="44" author="Eversis">
        <loadData file="db/init-data/page.csv" tableName="page" separator="|" />
    </changeSet>
    <changeSet id="45" author="Eversis">
        <loadData file="db/init-data/page-title.csv" tableName="page_title" separator="|" />
    </changeSet>
    <changeSet id="46" author="Eversis">
        <loadData file="db/init-data/page-description.csv" tableName="page_description" separator="|" />
    </changeSet>
    <changeSet id="47" author="Eversis">
        <loadData file="db/init-data/menu.csv" tableName="menu" separator="|" />
    </changeSet>
    <changeSet id="48" author="Eversis">
        <loadData file="db/init-data/menu-title.csv" tableName="menu_title" separator="|" />
    </changeSet>
    <changeSet id="49" author="Eversis">
        <loadData file="db/init-data/menu-image-title.csv" tableName="menu_image_title" separator="|" />
    </changeSet>
</databaseChangeLog>