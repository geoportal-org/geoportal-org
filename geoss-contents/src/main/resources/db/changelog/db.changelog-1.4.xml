<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.17.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="24" author="Eversis">
        <comment>Clear any existing legacy content data in the tables and populate with init data</comment>
        <sql splitStatements="true">
            TRUNCATE TABLE content;
            TRUNCATE TABLE content_aud;
        </sql>
        <loadData file="db/init-data/content.csv" tableName="content" separator="|" >
            <column header="id" name="id" type="BIGINT(20)"/>
            <column header="title" name="title" type="VARCHAR(255)"/>
            <column header="data" name="data" type="LONGTEXT"/>
            <column header="published" name="published" type="BOOLEAN"/>
            <column header="created_by" name="created_by" type="VARCHAR(255)"/>
            <column header="created_on" name="created_on" type="DATETIME"/>
            <column header="modified_by" name="modified_by" type="VARCHAR(255)"/>
            <column header="modified_on" name="modified_on" type="DATETIME"/>
        </loadData>
    </changeSet>
    <changeSet id="25" author="Eversis">
        <comment>Clear any existing legacy page data in the tables and populate with init data</comment>
        <sql splitStatements="true">
            TRUNCATE TABLE page;
            TRUNCATE TABLE page_aud;
        </sql>
        <loadData file="db/init-data/page.csv" tableName="page" separator="|" />
    </changeSet>
    <changeSet id="26" author="Eversis">
        <comment>Clear any existing legacy menu data in the tables and populate with init data</comment>
        <sql splitStatements="true">
            TRUNCATE TABLE menu;
            TRUNCATE TABLE menu_aud;
        </sql>
        <loadData file="db/init-data/menu.csv" tableName="menu" separator="|" />
    </changeSet>
</databaseChangeLog>